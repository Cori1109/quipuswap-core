{
  "name": "quipuswap",
  "version": "0.0.1",
  "config": {
    "network": "http://127.0.0.1:8732",
    "network1": "https://testnet-tezos.giganode.io",
    "standard": "fa1.2",
    "default": {
      "key": "../../fixtures/key",
      "storagedir": "./storage",
      "inputdir": "./build",
      "pool": {
        "tez": "1.0",
        "token": "1000"
      }
    }
  },

  "scripts": {
    "clean": "rm -rf ./build",
    "compile": "truffle compile",
    "start-sandbox": "ganache-cli --flavor tezos --seed alice",
    "build": "./scripts/build.sh",
    "build_fa1.2": "./scripts/build_fa1.2.sh",
    "build-fa2": "./scripts/build_fa2.sh",
    "deploy": "./scripts/deploy.sh",
    "deploy-fa1.2": "./scripts/deploy_fa1.2.sh",
    "deploy-fa2": "./scripts/deploy_fa2.sh",
    "set-functions": "./scripts/set_functions.sh",
    "set-dex-functions-fa1.2": "./scripts/set_dex_functions_fa1.2.sh",
    "set-token-functions-fa1.2": "./scripts/set_token_functions_fa1.2.sh",
    "set-functions-fa2": "./scripts/set_functions_fa2.sh",
    "add-tokens": "node scripts/cli.js add_token $npm_package_config_default_pool_tez $npm_package_config_default_pool_token -p $npm_package_config_network && node scripts/cli.js add_token $npm_package_config_default_pool_tez $npm_package_config_default_pool_token -p $npm_package_config_network Token2",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './test/*.spec.ts'"
  },
  "dependencies": {
    "@taquito/taquito": "^6.3.4-beta.0",
    "bignumber.js": "^9.0.0",
    "commander": "^5.1.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "^6.10.0-tezos.1",
    "typescript": "^4.0.3"
  },
  "directories": {
    "test": "./test"
  },
  "devDependencies": {
    "ganache-cli": "^6.10.0-tezos.1",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "ts-node": "^9.0.0"
  }
}
