{
  "name": "quipuswap",
  "version": "0.0.1",
  "config": {
    "standard": "FA12"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "migrate": "npm run clean && truffle migrate",
    "migrate-carthagenet": "truffle migrate --network carthagenet",
    "migrate-delphinet": "truffle migrate --network delphinet",
    "migrate-delphinet-reset": "truffle migrate --network delphinet --reset",
    "compile-dex": "node scripts/cli.js build ./main/Dex$npm_package_config_standard --no-json -o contracts",
    "compile": "npm run clean && node scripts/cli.js build ./main/Dex$npm_package_config_standard --no-json -o contracts && truffle compile",
    "start-sandbox": "ganache-cli --flavor tezos --seed alice",
    "test": "tsc  && npm run clean && npm run compile && truffle test",
    "test:quick": "tsc && truffle test"
  },
  "dependencies": {
    "@taquito/taquito": "^6.3.4-beta.0",
    "bignumber.js": "^9.0.0",
    "commander": "^5.1.0",
    "dotenv": "^8.2.0",
    "ganache-cli": "^6.10.0-tezos.1",
    "typescript": "^4.0.3",
    "truffle": "^5.5.0-tezos.4"
  },
  "directories": {
    "test": "./test"
  },
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "ts-node": "^9.0.0"
  }
}
